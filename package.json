{
  "name": "hexagonal-tasks",
  "description": "API de notas utilizando arquitectura hexagonal",
  "version": "0.0.1",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/franco4457/hexagonal-tasks.git"
  },
  "keywords": [
    "hexagonal",
    "typescript",
    "api"
  ],
  "author": "franco4457",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.prod.json && tsc-alias",
    "start:express": "node build/src/index.js",
    "dev:express": "nodemon --exec ts-node --esm ./src/index.ts",
    "lint": "eslint --ext .jsx,.js,.ts,.tsx .",
    "typecheck": "tsc --noEmit",
    "test": "vitest"
  },
  "devDependencies": {
    "@swc/core": "^1.3.80",
    "@types/express": "^4.17.17",
    "@types/node": "^20.5.6",
    "@types/supertest": "^2.0.15",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "eslint": "^8.48.0",
    "eslint-config-standard-with-typescript": "^39.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.0.2",
    "eslint-plugin-promise": "^6.1.1",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.7",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.3"
  },
  "dependencies": {
    "express": "^4.18.2"
  }
}
